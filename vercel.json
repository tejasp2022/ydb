{
    "builds": [
        {
            "src": "landing_page/package.json",
            "use": "@vercel/next"
        },
        {
            "src": "podcast_generation/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "landing_page/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "podcast_generation/main.py"
        }
    ],
    "functions": {
        "podcast_generation/main.py": {
          "runtime": "@vercel/python",
          "entrypoint": "main.py"
        }
    }
}