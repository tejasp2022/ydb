{
    "builds": [
        {
            "src": "landing_page/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "landing_page/$1"
        }
    ],
    "functions": {
        "podcast_generation/api/*.py": {
            "runtime": "python3.12"
        }
    }
}